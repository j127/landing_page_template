{
    "name": "parcel_example",
    "version": "1.1.0",
    "license": "Unlicense",
    "scripts": {
        "format": "prettier --write '**/*.{md,mdx,ts,js,jsx,tsx,yaml,yml,json,html,css,scss}'",
        "clean": "rm -rf ./dist/*",
        "start": "yarn clean && parcel ./src/index.html",
        "build": "yarn clean && parcel build --no-source-maps ./src/index.html",
        "deploy:staging": "yarn build && netlify deploy --dir=dist",
        "deploy": "yarn build && netlify deploy --prod --dir=dist",
        "netlify:login": "netlify login",
        "netlify:link": "netlify link",
        "prepare": "husky"
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,json,md,mdx,yml,yaml,html,css,scss}": [
            "yarn format"
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "@parcel/transformer-sass": "^2.11.0",
        "@types/node": "^20.11.19",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "netlify-cli": "^17.16.4",
        "parcel": "^2.11.0",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "prettier": "^3.2.5",
        "sass": "^1.71.1",
        "typescript": "^5.3.3"
    }
}
